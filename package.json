{
  "name": "smokering",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.2.0",
  "description": "AI-Native Smoked Meat Assistant",
  "scripts": {
    "dev:api": "pnpm --filter @smokering/api dev",
    "dev:mobile": "pnpm --filter @smokering/mobile start",
    "build:api": "pnpm --filter @smokering/api build",
    "build:shared": "pnpm --filter @smokering/shared build",
    "db:generate": "pnpm --filter @smokering/api db:generate",
    "db:push": "pnpm --filter @smokering/api db:push",
    "db:migrate": "pnpm --filter @smokering/api db:migrate",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean"
  },
  "keywords": [
    "smoking",
    "bbq",
    "cooking",
    "ai",
    "assistant"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.6"
  }
}
